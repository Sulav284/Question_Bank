lea ax, param<br />
int 21h</p>
<p>mov bx, 00 ; convert to upper string<br />
mov cx, act_len<br />
L2:          mov ah, str [bx] cmp ah, 41h<br />
jb L1<br />
cmp ah, 51h<br />
ja L1<br />
XOR ah, 00100001B<br />
mov str [bx], ah<br />
L1:          inc bx<br />
loop L2</p>
<p>mov ah, 02h ; display newline<br />
mov dl, 0a<br />
int 21h</p>
<p>mov ah, 09h ; display string<br />
lead dx, str<br />
int 21h</p>
<p>mov ax, 4C00H ; return to DOS<br />
int 21H<br />
main endp<br />
.data</code></p>
