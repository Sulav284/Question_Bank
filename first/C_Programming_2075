<p><strong>Program Part</strong></p>
<pre class="EnlighterJSRAW" data-enlighter-language="generic">#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;

int main(){

    FILE *fptr1, *fptr2; 
    char c;

    // Open One file for reading 
    fptrl = fopen("input.txt", "r");
    
    if( fptrl == NULL ){
        printf("Cannot open a file");
        exit(0);
    }

    //opeaning another file for writing
    fptr2 = fopen("output.txt", "w");
    if( fptr2 == NULL ){
        printf("Cannot open a file");
        exit(0);
    }

    //Read contents from file
    c = fgets(fptr1);

    while(c != EOF){
        fputc(c, fptr2);
        c = fgetc(fptr1)''
    }

    printf("\nContents copied");
    fclose(fptr1);
    fclose(fptr2);
    
    return 0;

}</pre>
